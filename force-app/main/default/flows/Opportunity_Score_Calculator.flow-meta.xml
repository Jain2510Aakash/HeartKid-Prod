    <description>Calculates Opportunity score based on what we have extracted</description>
    <environments>Default</environments>
    <formulas>
        <description>If Question 57 is true then 10 otherwise calculate by using static question &quot;Automation&quot;.</description>
        <name>Automation_Label</name>
        <dataType>Number</dataType>
        <expression>IF(
    {!Interrogation_Question_Int_0001.Value__c} = &apos;true&apos;, 
    10,
    IF(
        {!Assist_Question_Answer.Opportunity_Q_6__c} = 0, 
        0,
        IF(
            AND({!Assist_Question_Answer.Opportunity_Q_6__c} &gt; 0, {!Assist_Question_Answer.Opportunity_Q_6__c} &lt;= 3),
            2,
            IF(
		 AND({!Assist_Question_Answer.Opportunity_Q_6__c} &gt; 3, {!Assist_Question_Answer.Opportunity_Q_6__c} &lt;= 5),
                4,
                IF(
			AND({!Assist_Question_Answer.Opportunity_Q_6__c} &gt; 5, {!Assist_Question_Answer.Opportunity_Q_6__c} &lt;= 8),
			5,
			IF(
				AND({!Assist_Question_Answer.Opportunity_Q_6__c} &gt; 8, {!Assist_Question_Answer.Opportunity_Q_6__c} &lt;= 10),
				4,
				IF(
					{!Assist_Question_Answer.Opportunity_Q_6__c} &gt; 10,
					1,
					Null
					 )
					)
				)
            )
        )
    )
)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>&quot;suppose result:
result = 0 THEN score = 10
0&lt;result&lt;=2 THEN score = 7
2&lt;result&lt;=4 THEN score = 4
result&gt;=5 THEN score = 0&quot;</description>
        <name>BusinessPorcess_And_RecordType_Result</name>
        <dataType>Number</dataType>
        <expression>IF(
    {!BusinessProcess_RecordType} = 0, 
    10,
    IF(
        AND({!BusinessProcess_RecordType} &gt; 0, {!BusinessProcess_RecordType} &lt;= 2), 
        7,
        IF(
            AND({!BusinessProcess_RecordType} &gt; 2, {!BusinessProcess_RecordType} &lt;= 4), 
            4,
            IF(
                {!BusinessProcess_RecordType} &gt;= 5, 
                0,
                null
            )
        )
    )
)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>No. of Business Process - No. of Record Type</description>
        <name>BusinessProcess_RecordType</name>
        <dataType>Number</dataType>
        <expression>ABS({!Assist_Question_Answer.Opportunity_Q_5__c}-value({!Interrogation_Question_Int_0000.Value__c}))</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>If donation information is true then 10, otherwise 0</description>
        <name>Donation_Information</name>
        <dataType>Number</dataType>
        <expression>IF({!Interrogation_Question_Int_0030.Value__c} = &apos;true&apos;, 10, 0)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>If Donation record type is true then 10 otherwise 0.</description>
        <name>Donation_RecordType</name>
        <dataType>Number</dataType>
        <expression>IF({!Interrogation_Question_Int_0010.Value__c} = &apos;true&apos;, 10, 0)</expression>
        <scale>0</scale>
    </formulas>
    <formulas>
        <description>How do you manage Duplicate Opportunity?</description>
        <name>If_Duplicate_Rule</name>
        <dataType>Number</dataType>
        <expression>IF(
    {!Assist_Question_Answer.Opportunity_Q_8__c} = 0, 
    0,
    IF(
        {!Assist_Question_Answer.Opportunity_Q_8__c} = 1, 
        7,
        IF(
            {!Assist_Question_Answer.Opportunity_Q_8__c} = 2, 
            10,
            IF(
				{!Assist_Question_Answer.Opportunity_Q_8__c} = 3,
                7,
                IF(
					{!Assist_Question_Answer.Opportunity_Q_8__c} &gt;= 4,
					0,
					Null
				)
            )
        )
    )
)</expression>
        <scale>0</scale>
    </formulas>

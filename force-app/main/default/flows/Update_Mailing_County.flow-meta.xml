        <locationX>176</locationX>
        <locationY>311</locationY>
        <inputAssignments>
            <field>MailingCountry</field>
            <value>
                <stringValue>Australia</stringValue>
            </value>
        </inputAssignments>
        <inputReference>$Record</inputReference>
    </recordUpdates>
    <start>
